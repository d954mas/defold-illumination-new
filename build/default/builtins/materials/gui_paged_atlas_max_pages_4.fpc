    else if (page_index == 3) return texture2D(texture_sampler_3, dm_texture_array_args.st);
   return vec4(0.0);
}

void main()
{
    lowp vec4 tex = texture2DArray_texture_sampler(vec3(var_texcoord0.xy, var_page_index));
    gl_FragColor = tex * var_color;
